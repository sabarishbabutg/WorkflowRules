<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<Dialog id="id_massagent" class="cl_massconfirmationDialog sapUiSizeCompact sapUiSmallMargin" contentWidth ="380px">
		<customHeader>
			<Toolbar class="cl_ConfirmationToolbar" height="2.5rem">
				<ToolbarSpacer/>
				<Text text="{i18n>MAC}" class="cl_ConfirmationTitle sapUiTinyMarginTopBottom"/>
				<ToolbarSpacer/>
			</Toolbar>
		</customHeader>
		
		<VBox class="sapUiSmallMargin">
			<RadioButtonGroup id="ID_MassAgent" select="onMassAgentOptionSelect" columns="3" class="cl_wfradiobtns">
			<RadioButton text="{i18n>ApplicationWise}"/>
				<RadioButton text="{i18n>MasterWise}"/>
				<RadioButton text="{i18n>Overall}"/>
		</RadioButtonGroup>
		<VBox id="dynamicAgentBox" class="sapUiSmallMarginTop"></VBox>
		</VBox>
		<buttons>
			<Button text="Ok" press="fnsubmitMassAgentChange" icon="{= ${JM_ImageModel>/path} + 'Apply.svg' }" class="cl_primaryButton sapUiSmallMarginEnd"/>
			<Button text="Cancel" class="cl_secondaryButton" icon="{= ${JM_ImageModel>/path} + 'Cancel.svg' }" press="fnCancelMassAgentChange"/>
		</buttons>
	</Dialog>
</core:FragmentDefinition>