{
	"_version": "1.7.0",
	"sap.app": {
		"id": "WorkflowRules",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "servicecatalog.connectivityComponent",
			"version": "0.0.0"
		},
		"dataSources": {
			"MDM_CONFIG_SRV": {
				"uri": "/sap/opu/odata/exl/MDM_CONFIG_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/MDM_CONFIG_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "WorkflowRules.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "WorkflowRules.i18n.i18n"
				}
			},
			"JMConfig": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"settings": {
					"defaultOperationMode": "Server",
					"defaultBindingMode": "OneTime",
					"defaultCountMode": "Request"
				},
				"dataSource": "MDM_CONFIG_SRV",
				"preload": true
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				},
				{
					"uri": "css/Workflow.css"
				},
				{
					"uri": "css/RulesEngine.css"
				},
				{
					"uri": "css/Home.css"
				}
			]
		},
		"routing": {
			"config": {
				"viewPath": "WorkflowRules.view",
				"routerClass": "sap.m.routing.Router",
				"controlId": "id_app"
			},
			"targets": {
				"HomePage": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "HomePage",
					"viewLevel": 1,
					"controlAggregation": "pages"
				},
				"Workflow": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "workflow",
					"viewLevel": 2,
					"controlAggregation": "pages"
				},
				"RulesEngine": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "RulesEngine",
					"viewLevel": 3,
					"controlAggregation": "pages"
				}
			},
			"routes": [
				{
					"name": "HomePage",
					"pattern": "",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"HomePage"
					]
				},
				{
					"name": "Workflow",
					"pattern": "Workflow",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"Workflow"
					]
				},
				{
					"name": "RulesEngine",
					"pattern": "RulesEngine",
					"titleTarget": "",
					"greedy": false,
					"target": [
						"RulesEngine"
					]
				}
			]
		}
	}
}